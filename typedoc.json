{
    "entryPoints": ["lib/**/*.ts"],
    "entryPointStrategy": "expand",
    "out": "docs/api",
    "theme": "default",
    "exclude": [
        "**/node_modules/**",
        "**/dist/**",
        "**/tmp/**",
        "**/test/**",
        "**/example/**"
    ],
    "includeVersion": true,
    "excludePrivate": false,
    "excludeProtected": false,
    "excludeExternals": true,
    "readme": "README.md",
    "name": "@hrefcl/apidoc",
    "gitRevision": "main",
    "gitRemote": "origin",
    "projectDocuments": ["md/en/*.md", "md/es/*.md"],
    "navigationLinks": {
        "GitHub": "https://github.com/hrefcl/apidoc",
        "NPM": "https://www.npmjs.com/package/@hrefcl/apidoc"
    },
    "sidebarLinks": {
        "Documentation": "https://apidoc.app",
        "Live Demo": "http://apidocts.com/example/"
    },
    "categorizeByGroup": true,
    "defaultCategory": "Other",
    "categoryOrder": [
        "Core",
        "Parser",
        "Filter",
        "Worker",
        "Language",
        "Utility",
        "*"
    ],
    "sort": ["source-order"],
    "kindSortOrder": [
        "Reference",
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "EnumMember",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Property",
        "Variable",
        "Function",
        "Accessor",
        "Method",
        "Parameter",
        "TypeParameter",
        "TypeLiteral",
        "CallSignature",
        "ConstructorSignature",
        "IndexSignature",
        "GetSignature",
        "SetSignature"
    ],
    "validation": {
        "notExported": true,
        "invalidLink": false,
        "notDocumented": false
    },
    "treatWarningsAsErrors": false,
    "intentionallyNotExported": [
        "ApiDocOptions",
        "DefaultConfig",
        "LoggerInterface",
        "MarkdownParser",
        "ApiDocParseResult",
        "AppContext",
        "OpenApiSpec",
        "ApiDocCompatibleResult",
        "ParsedInterface",
        "Config",
        "ApiDocItem",
        "ProjectData",
        "ParsedFile",
        "PackageInfos"
    ],
    "requiredToBeDocumented": [
        "Enum",
        "EnumMember",
        "Variable",
        "Function",
        "Class",
        "Interface",
        "Property",
        "Method",
        "Accessor",
        "TypeAlias"
    ],
    "highlightLanguages": [
        "typescript",
        "javascript",
        "java",
        "clojure",
        "coffeescript",
        "elixir",
        "erlang",
        "perl",
        "python",
        "ruby",
        "php",
        "go",
        "rust",
        "dart",
        "scala",
        "bash",
        "shell",
        "dockerfile",
        "nginx",
        "apache",
        "makefile",
        "yaml",
        "json",
        "xml",
        "mermaid"
    ],
    "searchInComments": true,
    "searchInDocuments": true,
    "blockTags": [
        "@api",
        "@apiBody",
        "@apiDeprecated",
        "@apiDescription",
        "@apiError",
        "@apiExample",
        "@apiGroup",
        "@apiHeader",
        "@apiName",
        "@apiParam",
        "@apiSampleRequest",
        "@apiSchema",
        "@apiSuccess",
        "@apiUse",
        "@apiVersion",
        "@openapi",
        "@openapi-operation",
        "@openapi-path",
        "@openapi-schema",
        "@mqtt",
        "@mqttGroup",
        "@topic",
        "@topicParam",
        "@payload",
        "@payloadSchema",
        "@qos",
        "@retain",
        "@auth",
        "@errors",
        "@examplePublish",
        "@exampleSubscribe",
        "@responseExample",
        "@responseTopic",
        "@ratelimit",
        "@tags",
        "@file",
        "@package",
        "@version",
        "@copyright",
        "@description",
        "@hook",
        "@priority",
        "@performance",
        "@resilient",
        "@memberof",
        "@author",
        "@license",
        "@see",
        "@class",
        "@example",
        "@since",
        "@public",
        "@param",
        "@returns",
        "@internal",
        "@throws",
        "@interface"
    ],
    "modifierTags": [
        "@api",
        "@apiDefine",
        "@apiStructure"
    ]
}