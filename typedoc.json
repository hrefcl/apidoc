{
  "blockTags": [
    "@api",
    "@apiBody",
    "@apiDeprecated",
    "@apiDescription",
    "@apiError",
    "@apiExample",
    "@apiGroup",
    "@apiHeader",
    "@apiName",
    "@apiParam",
    "@apiSampleRequest",
    "@apiSchema",
    "@apiSuccess",
    "@apiUse",
    "@apiVersion",
    "@openapi",
    "@openapi-operation",
    "@openapi-path",
    "@openapi-schema",
    "@mqtt",
    "@mqttGroup",
    "@topic",
    "@topicParam",
    "@payload",
    "@payloadSchema",
    "@qos",
    "@retain",
    "@auth",
    "@errors",
    "@examplePublish",
    "@exampleSubscribe",
    "@responseExample",
    "@responseTopic",
    "@ratelimit",
    "@tags",
    "@file",
    "@package",
    "@version",
    "@copyright",
    "@description",
    "@hook",
    "@priority",
    "@performance",
    "@resilient",
    "@memberof",
    "@author",
    "@license",
    "@see",
    "@class",
    "@example",
    "@since",
    "@public",
    "@param",
    "@returns",
    "@internal",
    "@throws",
    "@interface"
  ],
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Parser", "Filter", "Worker", "Language", "Utility", "*"],
  "defaultCategory": "Other",
  "entryPointStrategy": "expand",
  "entryPoints": ["lib/**/*.ts"],
  "exclude": ["**/node_modules/**", "**/dist/**", "**/tmp/**", "**/test/**", "**/example/**"],
  "excludeExternals": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "gitRemote": "origin",
  "gitRevision": "main",
  "highlightLanguages": [
    "typescript",
    "javascript",
    "java",
    "clojure",
    "coffeescript",
    "elixir",
    "erlang",
    "perl",
    "python",
    "ruby",
    "php",
    "go",
    "rust",
    "dart",
    "scala",
    "bash",
    "shell",
    "dockerfile",
    "nginx",
    "apache",
    "makefile",
    "yaml",
    "json",
    "xml",
    "mermaid"
  ],
  "includeVersion": true,
  "intentionallyNotExported": [
    "ApiDocOptions",
    "DefaultConfig",
    "LoggerInterface",
    "MarkdownParser",
    "ApiDocParseResult",
    "AppContext",
    "OpenApiSpec",
    "ApiDocCompatibleResult",
    "ParsedInterface",
    "Config",
    "ApiDocItem",
    "ProjectData",
    "ParsedFile",
    "PackageInfos"
  ],
  "kindSortOrder": [
    "Reference",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Accessor",
    "Method",
    "Parameter",
    "TypeParameter",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "GetSignature",
    "SetSignature"
  ],
  "modifierTags": ["@api", "@apiDefine", "@apiStructure"],
  "name": "@hrefcl/apidoc",
  "navigationLinks": {
    "GitHub": "https://github.com/hrefcl/apidoc",
    "NPM": "https://www.npmjs.com/package/@hrefcl/apidoc"
  },
  "out": "docs/api",
  "projectDocuments": ["md/en/*.md", "md/es/*.md"],
  "readme": "README.md",
  "requiredToBeDocumented": ["Enum", "EnumMember", "Variable", "Function", "Class", "Interface", "Property", "Method", "Accessor", "TypeAlias"],
  "searchInComments": true,
  "searchInDocuments": true,
  "sidebarLinks": {
    "Documentation": "https://apidoc.app",
    "Live Demo": "http://apidocts.com/example/"
  },
  "sort": ["source-order"],
  "theme": "default",
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": false,
    "notDocumented": false
  }
}
